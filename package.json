{  "name": "unpar-task-management",  "version": "1.0.0",  "description": "UNPAR Task Management System",  "scripts": {    "start": "concurrently \"npm run server\" \"npm run client\"",    "server": "cd server && npm run dev",    "client": "cd client && npm start",    "install-all": "npm install && cd client && npm install && cd ../server && npm install"  },  "dependencies": {    "concurrently": "^8.0.1"  }}